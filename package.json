{
  "name": "prettignore",
  "version": "2.1.3",
  "description": "Prettier but for ignore files",
  "keywords": [
    "ignore",
    "format",
    "gitignore"
  ],
  "homepage": "https://github.com/papaia/prettignore#readme",
  "bugs": {
    "url": "https://github.com/papaia/prettignore/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papaia/prettignore.git"
  },
  "license": "MIT",
  "author": "Papaia",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "prettignore": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc && add-shebang",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "ava"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  },
  "dependencies": {
    "globby": "^11.0.2",
    "minimist": "^1.2.5",
    "outdent": "^0.8.0",
    "time-span": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@papaia/prettier": "2.2.0",
    "@papaia/tsconfig": "1.0.1",
    "@types/minimist": "1.2.1",
    "@types/node": "14.14.28",
    "add-shebang": "0.1.0",
    "ava": "3.15.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "micromatch": "4.0.2",
    "pinst": "2.1.4",
    "prettier": "2.2.1",
    "prettier-plugin-packagejson": "2.2.9",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  }
}
